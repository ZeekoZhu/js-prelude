{
  "name": "importify",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "zeeko",
  "sourceRoot": "apps/importify/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-builders/custom-webpack:browser",
      "outputs": ["{options.outputPath}"],
      "inputs": [
        "default",
        {
          "env": "APP_CLIENT_ID"
        },
        {
          "env": "APP_REDIRECT_URL"
        }
      ],
      "options": {
        "customWebpackConfig": {
          "path": "apps/importify/webpack.config.js"
        },
        "outputPath": "dist/apps/importify",
        "index": "apps/importify/src/index.html",
        "main": "apps/importify/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/importify/tsconfig.app.json",
        "assets": [
          "apps/importify/src/favicon.ico",
          "apps/importify/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/deeppurple-amber.css",
          "apps/importify/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/importify/src/environments/environment.ts",
              "with": "apps/importify/src/environments/environment.development.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "importify:build:production"
        },
        "development": {
          "browserTarget": "importify:build:development",
          "allowedHosts": ["importify-dev.zeeko.dev"],
          "hmr": true
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "importify:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/importify/**/*.ts",
          "apps/importify/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/importify/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "importify:build"
      }
    }
  }
}
